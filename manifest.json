{
  "short_name": "Widget Editor Themes",
  "name": "Widget Editor Themes",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Widget Editor Themes"
  },
  "background": {
    "service_worker": "src/background.ts"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["https://*/*"],
  "icons": {
    "128": "logo.png"
  },
  "version": "1.0.0"
}
